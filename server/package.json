    "dotenv-cli": "^11.0.0",
    "jest": "^30.2.0",
    "prisma": "^7.1.0",
    "ts-jest": "^29.4.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
